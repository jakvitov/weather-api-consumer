{
  "openapi":"3.0.1",
  "info":{
    "title":"OpenAPI definition",
    "version":"v0"
  },
  "servers":[
    {
      "url":"http://localhost:8080",
      "description":"Generated server url"
    }
  ],
  "paths":{
    "/forecast/geocodes":{
      "get":{
        "tags":[
          "weather-info-controller"
        ],
        "operationId":"customDaysWeatherResponse",
        "parameters":[
          {
            "name":"latitude",
            "in":"query",
            "required":true,
            "schema":{
              "type":"number",
              "format":"double"
            }
          },
          {
            "name":"longitude",
            "in":"query",
            "required":true,
            "schema":{
              "type":"number",
              "format":"double"
            }
          },
          {
            "name":"days",
            "in":"query",
            "required":false,
            "schema":{
              "type":"integer",
              "format":"int32"
            }
          }
        ],
        "responses":{
          "200":{
            "description":"OK",
            "content":{
              "*/*":{
                "schema":{
                  "$ref":"#/components/schemas/WeatherForecastResponseDto"
                }
              }
            }
          }
        }
      }
    },
    "/forecast/cityname":{
      "get":{
        "tags":[
          "weather-info-controller"
        ],
        "operationId":"getForecastForCityName",
        "parameters":[
          {
            "name":"city",
            "in":"query",
            "required":true,
            "schema":{
              "type":"string"
            }
          },
          {
            "name":"days",
            "in":"query",
            "required":false,
            "schema":{
              "type":"integer",
              "format":"int32"
            }
          }
        ],
        "responses":{
          "200":{
            "description":"OK",
            "content":{
              "*/*":{
                "schema":{
                  "$ref":"#/components/schemas/WeatherForecastResponseDto"
                }
              }
            }
          }
        }
      }
    }
  },
  "components":{
    "schemas":{
      "WeatherForecastResponseDto":{
        "required":[
          "days_forecast"
        ],
        "type":"object",
        "properties":{
          "generated_at":{
            "type":"string",
            "format":"date-time"
          },
          "days_forecast":{
            "type":"array",
            "items":{
              "$ref":"#/components/schemas/WeatherInfoForDayDto"
            }
          }
        }
      },
      "WeatherInfoForDayDto":{
        "required":[
          "date",
          "hourly_info"
        ],
        "type":"object",
        "properties":{
          "date":{
            "type":"string",
            "format":"date"
          },
          "hourly_info":{
            "type":"array",
            "items":{
              "$ref":"#/components/schemas/WeatherInfoForHourInfoDto"
            }
          }
        }
      },
      "WeatherInfoForHourInfoDto":{
        "required":[
          "rain",
          "temperature",
          "time",
          "weather_name",
          "windspeed"
        ],
        "type":"object",
        "properties":{
          "time":{
            "type":"string",
            "format":"date-time"
          },
          "temperature":{
            "type":"number",
            "format":"double"
          },
          "windspeed":{
            "type":"number",
            "format":"double"
          },
          "rain":{
            "minimum":0,
            "type":"number",
            "format":"double"
          },
          "weather_name":{
            "type":"string",
            "enum":[
              "CLEAR_SKY",
              "MAINLY_CLEAR",
              "PARTLY_CLOUDY",
              "OVERCAST",
              "FOG",
              "DESPOSING_FOG",
              "LIGHT_DRIZZLE",
              "MODERATE_DRIZZLE",
              "DENSE_DRIZZLE",
              "LIGHT_FREEZING_DRIZZLE",
              "HEAVY_FREEZING_DRIZZLE",
              "LIGHT_RAIN",
              "MEDIUM_RAIN",
              "HEAVY_RAIN",
              "LIGHT_FREEZING_RAIN",
              "HEAVY_FREEZING_RAIN",
              "LIGHT_SNOW",
              "MODERATE_SNOW",
              "HEAVY_SNOW",
              "SNOW_GRAINS",
              "LIGHT_RAIN_SHOWER",
              "MODERATE_RAIN_SHOWER",
              "HEAVY_RAIN_SHOWER",
              "LIGHT_SNOW_SHOWER",
              "HEAVY_SNOW_SHOWER",
              "THUNDERSTORM",
              "THUNDERSTORM_LIGHT_HAIL",
              "THUNDERSTORM_HEAVY_HAIL"
            ]
          }
        }
      }
    }
  }
}